{
    "compilerOptions": {
      "downlevelIteration": true,
      "target": "es5",
      "lib": ["dom", "dom.iterable", "esnext"],
      "allowJs": true,
      "skipLibCheck": true, 
      "strict": true,
      "forceConsistentCasingInFileNames": true,
      "noEmit": true,
      "esModuleInterop": true,
      "module": "esnext",
      "moduleResolution": "node",
      "resolveJsonModule": true,
      "isolatedModules": true,
      "jsx": "preserve",
      "incremental": true,
      "baseUrl": "src/",
            "paths": {
        "@/components/*": ["components/*"],
        "@/pages/*": ["pages/*"],
        "@/styles/*": ["styles/*"]
      }
    },
    "include": ["src","next-env.d.ts", "**/*.ts", "**/*.tsx", "types.d.ts", "__tests__/navBar.test.tsx"], 
    "exclude": ["node_modules"]
  }
  